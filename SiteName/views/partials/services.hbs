    <section id={{this.alias}}>
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">{{this.content.default.title}}</h2>
            <hr class="my-4">
          </div>
        </div>
      </div>
      <div class="container">
        <div class="row">
          {{#each this.children}}
            <div class="col-lg-3 col-md-6 text-center">
            <div class="service-box mt-5 mx-auto">
              <i class="fa fa-4x {{this.content.default.icon}} text-primary mb-3 sr-icons"></i>
              <h3 class="mb-3">{{this.content.default.text}}</h3>
              <p class="text-muted mb-0">{{this.content.default.subText}}</p>
            </div>
          </div>
          {{/each}}
        </div>
      </div>
    </section>